# Environment variables for JobNaut Production

# Database configuration (to be set with production database)
DATABASE_URL=postgresql://user:password@localhost:5432/jobnaut_prod

# Clerk configuration (to be set with production Clerk keys)
CLERK_SECRET_KEY=your_production_clerk_secret_key

# Application configuration
NODE_ENV=production
PORT=3000

