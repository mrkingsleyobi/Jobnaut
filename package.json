{
  "name": "jobnaut",
  "version": "1.0.0",
  "description": "JobNaut - AI-Powered Job Market Navigator",
  "main": "index.js",
  "scripts": {
    "dev": "node src/server.js",
    "start": "node src/server.js",
    "build": "prisma generate",
