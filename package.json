    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "node-cache": "^5.1.2",
    "winston": "^3.18.3",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "env-cmd": "^11.0.0",
